{
  "name": "utaku",
  "version": "0.1.03",
  "private": true,
  "description": "Utaku image downloader",
  "keywords": [
    "chrome-extension",
    "web-extension",
    "typescript",
    "react",
    "rollup",
    "boilerplate"
  ],
  "scripts": {
    "build": "rollup -c",
    "prod": "rimraf build && rimraf releases/$npm_package_name-$npm_package_version && cross-env NODE_ENV=production rollup -c && mkdir -p releases/$npm_package_name-$npm_package_version && mv build/* releases/$npm_package_name-$npm_package_version && rimraf build",
    "start": "rollup -c -w",
    "dev": "cross-env NODE_ENV=watch webpack-dev-server --hot"
  },
  "author": "keisokoo",
  "license": "MIT",
  "dependencies": {
